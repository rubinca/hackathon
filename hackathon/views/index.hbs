<div class="page-container">

<form method="POST" class="form-inline">
  <div class="form-group" id="searchbar">
    <label for="search">Search</label>
    <input name="search" type="text" class="form-control" id="search" placeholder="Enter your search">
  </div>
  <button type="submit" class="btn btn-primary">Search</button>
</form>
<div>
	{{#each soundcloud}}
<iframe width="70%" height="100" scrolling="no" frameborder="no" src="https://w.soundcloud.com/player/?url=https%3A//api.soundcloud.com/tracks/
{{this.id}}&amp;auto_play=false&amp;hide_related=false&amp;show_comments=true&amp;show_user=true&amp;show_reposts=false&amp;
visual=true" style="display: block; float: left"></iframe>
	{{/each}}
</div>
<div>
{{#each youtube}}
<iframe id="ytplayer" type="text/html" width="70%" height="100"
  src="https://www.youtube.com/embed/{{this.id.videoId}}?autoplay=0&origin=http://example.com"
  frameborder="0"></iframe>
{{/each}}
</div>


</div>
